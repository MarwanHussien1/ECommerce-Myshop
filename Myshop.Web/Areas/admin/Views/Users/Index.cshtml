@model IEnumerable<ApplicationUser>



@{
	ViewBag.PageTitle = "Users Managment";
	ViewBag.CardTitle = "Users";
	Layout = "_Dashboard";
}

<a href="/Identity/Account/Register" class="btn btn-success">Create New User</a>

<table class="table table-hover" width="100%">
	<tr>
		<td>Name</td>
		<td>Email</td>
		<td>Name</td>
		<td>City</td>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.Name</td>
			<td>@item.Email</td>
			<td>@item.Name</td>
			<td>@item.city</td>
			<td>
				<a class="btn btn-danger" asp-action="LockUnlock">
					<i class="fas fa-lock-open"></i>
				</a>			
			</td>
		</tr>
	}
</table>